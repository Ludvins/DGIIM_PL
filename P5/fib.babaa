programa() {
  var
    entero n, aux;
  finvar

  entero fib (entero a)
  {
    si (a <= 1)
    {
    devolver 1;
    }
    devolver fib(a-1)+fib(a-2);
  }

  entrada "Introduce cuÃ¡ntos elementos de la serie de Fibonacci quieres: ", n;
  aux = 0;

  mientras(aux != n){

    salida fib(aux), " ";
    aux = aux+1;
  }
  salida "\n";

}
